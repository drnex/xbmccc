$(function() {
	//verticalTabs();
	$("#menu").tabs();
	$("button#play").button();
	//$("a#add").splitButton();
	$("#sort tbody").sortable().disableSelection();
});

verticalTabs = function(){
	$("#menu").tabs().addClass('ui-tabs-vertical ui-helper-clearfix');
    $("#menu li").removeClass('ui-corner-top').addClass('ui-corner-left');
};
