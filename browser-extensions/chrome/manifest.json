{
  "manifest_version": 2,

  "name": "XBMControlCenter",
  "description": "This extension provides some XBMC remote functions",
  "version": "1.0",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "tabs",
	"http://*/*"
  ],

  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["open.js"],
    "persistent": false
  }
}
